package com.test.day23递归;

public class Test5 {

	public static void main(String[] args) {
		//demo1();
		System.out.println(fun(47));
		demo1(47);
	}

	private static void demo1(int n) {
		long [] arr = new long[n];
		arr[0] = 1;
		arr[1] = 1;
		for (int i = 2; i < n; i++) {
			arr[i] = arr[i-2] + arr[i-1];		
			
		}
		System.out.println(arr[n - 1]);
	}
	public static long fun (int num){
		if(num == 1 || num == 2){
			return 1 ;
		}else{
			return fun(num - 2) + fun(num -1);
		}
	}
}
